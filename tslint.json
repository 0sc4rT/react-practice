{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-console": false,
    "interface-name": [true, "never-prefix"],
    "no-empty-interface": false,
    "grouped-imports": true,
    "object-literal-shorthand": false
  }
}
